{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next .wrangler .turbo dist",
    "build:prod": "pnpm run opennext:build && wrangler deploy --dry-run --outdir=dist",
    "opennext:build": "export SKIP_LINTER=false && opennextjs-cloudflare",
    "build:analyze": "ANALYZE=true next build",
    "deploy": "pnpm run opennext:build && wrangler deploy",
    "preview": "pnpm run opennext:build && wrangler dev",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate --name",
    "db:migrate:dev": "wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --local",
    "email:dev": "email dev -d src/react-email -p 3001"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.13",
    "@assistant-ui/react": "^0.7.88",
    "@assistant-ui/react-ai-sdk": "^0.7.16",
    "@assistant-ui/react-markdown": "^0.7.21",
    "@icons-pack/react-simple-icons": "^12.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@simplewebauthn/types": "^12.0.0",
    "@workspace/ui": "*",
    "ai": "^4.1.45",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.3",
    "ipaddr.js": "^2.2.0",
    "jotai": "^2.12.1",
    "lucide-react": "^0.475.0",
    "ms": "^2.1.3",
    "next": "^15.1.7",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@next/bundle-analyzer": "^15.1.7",
    "@opennextjs/cloudflare": "^0.5.3",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@workspace/configs": "*",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3",
    "wrangler": "^3.109.2"
  }
}
